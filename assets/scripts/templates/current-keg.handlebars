<h1> wtf handlebars</h1>
{{#each kegs}}


  <div class="row">
    <div class="panel panel-inverse edit-keg" data-attribute={{this.id}}>
      <div class="panel-body">
        <div class="col-md-6">
          <div class='keg-stats'>
            {{#if this.kicked}}
              <h5 class="keg-beer-name">The keg is kicked!</h5>
            {{else}}
              <h5 class="keg-beer-name">{{this.name}}</h5>
              <button class="kicked-btn" id="kicked-{{this.id}}" data-attribute="{{this.id}}">KICK IT</button>
            {{/if}}

          </div>
        </div>
        <div class="col-md-6">
        </div>
      </div>
    </div>
  </div><!--close row-->

      </div>
    </div>

{{/each}}
